{
  "name": "collective-instant",
  "version": "0.0.0",
  "description": "\"Instantly\" search Widen Media Collective for assets",
  "main": "index.js",
  "bin": {
    "collective-instant": "./bin/collective-instant"
  },
  "scripts": {
    "style": "jshint index.js",
    "test": "echo \"browserify test/all-test.js | testling\" && exit 0",
    "coverage": "echo \"browserify --debug -t coverify test/all-test.js | node | coverify\" && exit 0",
    "watch": "nodemon -w index.js -w lib -x 'npm run dev'",
    "dev": "browserify index.js --debug --standalone collective_instant | exorcist ./static/bundle.map.js > static/bundle.js",
    "build": "browserify index.js --standalone collective_instant > static/bundle.js && cat static/bundle.js | uglifyjs > static/bundle.min.js", "dev": "collective-oauth-server --collective-protocol http --collective-host localhost --collective-port 8080 -p 1337 --host localhost --static ./static",
    "start": "collective-oauth-server",
    "prepublish": "npm run build"
  },
  "author": "Mark Feltner",
  "license": "MIT",
  "devDependencies": {
    "browserify": "^7.0.0",
    "collective-oauth-server": "^1.0.1",
    "exorcist": "^0.1.6",
    "nodemon": "^1.2.1",
    "uglify-js": "^2.4.15"
  },
  "dependencies": {
    "bistre": "^1.0.1",
    "bole": "^1.0.0",
    "console-stream": "^0.1.1",
    "cookie-cutter": "^0.1.1",
    "detect-dom-ready": "^1.0.2",
    "media-collective": "^1.0.0",
    "minimist": "^1.1.0",
    "st": "^0.5.2",
    "uuid": "^2.0.1"
  }
}
